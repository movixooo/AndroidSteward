<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:src="@drawable/home_score_normal_bg"
        android:layout_centerInParent="true" />
    <azsecuer.androidy.com.azsecuer.azsecuer.android.com.Home.view.CustomProc
        android:layout_width="205dp"
        android:layout_height="205dp"
        android:id="@+id/customPrco"
        android:layout_centerInParent="true"
        app:roundColor="@color/white"
        app:roundProgressColor="@color/colorPrimary"
        app:roundWidth="8dp"
        app:textColor="@color/colorPrimaryDark"
        app:textSize="16sp"
        app:max="100"
        app:style="STOKE"

      />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textsize"
        android:textColor="#ffffff"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        android:gravity="center"
        android:background="@drawable/file_depth_accelerate"
        android:text="一键加速"
        android:layout_marginBottom="36dp"
        android:layout_alignBottom="@+id/customPrco"
        android:layout_centerHorizontal="true" />
</RelativeLayout>