<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="azsecuer.androidy.com.azsecuer">

    <!-- 杀死其他进程的权限 -->
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <!-- WIFI权限 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- 打电话权限 -->
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <!-- 相机权限 -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 外部SD卡的读写权限 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:label="@string/app_name"
        android:allowBackup="true"
        android:icon="@drawable/zhuomian"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".azsecuer.android.com.Home.activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".azsecuer.android.com.Home.activity.HomeActivity" />
        <activity android:name=".azsecuer.android.com.Home.activity.SettingActivity" />
        <activity android:name=".azsecuer.android.com.Home.activity.TelListActivity" />
        <activity android:name=".azsecuer.android.com.Home.activity.TelTypeActivity" />
        <activity android:name=".azsecuer.android.com.Home.activity.MobilelnfoActivity" />
