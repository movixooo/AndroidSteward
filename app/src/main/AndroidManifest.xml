        <activity android:name=".azsecuer.android.com.Home.activity.CleanActivity" />
        <activity android:name=".azsecuer.android.com.Home.activity.AccelerateActivity" />
        <activity android:name=".azsecuer.android.com.Home.activity.SoftwareActivity" />
        <activity android:name=".azsecuer.android.com.Home.activity.SoftwareManageActivity" />
        <activity android:name=".azsecuer.android.com.Home.activity.FileActivity" />
        <activity android:name=".azsecuer.android.com.Home.activity.FileBrowseActivity"></activity>
